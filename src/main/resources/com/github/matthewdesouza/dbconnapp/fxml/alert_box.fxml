<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="225.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.github.matthewdesouza.dbconnapp.view.AlertBoxController"
            stylesheets="@../css/font.css">
    <ImageView fitHeight="114.0" fitWidth="114.0" layoutX="12.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true"
               fx:id="alertImageView"/>
    <TextArea editable="false" layoutX="137.0" layoutY="13.0" prefHeight="200.0" prefWidth="200.0"
              fx:id="messageTextArea"/>
    <Button layoutX="49.0" layoutY="162.0" mnemonicParsing="false" text="Okay" onAction="#closeAlert"/>
</AnchorPane>
